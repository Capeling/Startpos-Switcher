{
	"geode": "4.0.0-beta.2",
	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074",
		"ios": "2.2074"
	},
	"id": "capeling.startpos_switcher",
	"name": "Startpos Switcher",
	"version": "v1.0.6",
	"developer": "Capeling",
	"description": "Literally just a startpos switcher",
	"settings": {
		"opacity":{
			"name": "Opacity",
			"default": 50,
			"max": 100,
			"min": 0,
			"control": {
				"slider": true,
				"slider-step": 1
			},
			"type": "float"
		},
		"hide":{
			"name": "Don't fade on start",
			"description": "Does not make the menu fade from max opacity when you first open a level",
			"default": false,
			"type": "bool"
		},
		"hideBtns":{
			"name": "Hide buttons",
			"description": "Hides the arrow buttons",
			"default": false,
			"type": "bool"
		},
		"ignoreDisabled": {
			"name": "Ignore Disabled",
			"description": "Ignores start positions that have been disabled",
			"default": false,
			"type": "bool",
			"platforms": [
				"win"
			]
		}
	},
	"links": {
		"source": "https://github.com/Capeling/Startpos-Switcher"
	},
	"issues": {
		"info": "If you have an issue with the mod please report it here!",
		"url": "https://github.com/Capeling/Startpos-Switcher/issues"
	},
	"tags": [
		"enhancement",
		"gameplay",
		"interface",
		"utility"
	],
	"dependencies": [
		{
			"id": "geode.custom-keybinds",
			"version": "v1.7.1",
			"importance": "required",
			"platforms": [
				"win"
			]
		}
	]
}
